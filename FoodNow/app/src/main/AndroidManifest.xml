<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ganesh.foodnow">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuList"
            android:label="@string/title_activity_menu_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RestrauntInfo"
            android:label="@string/title_activity_restraunt_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".mapActivity"
            android:label="@string/title_activity_map"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PlaceOrder"
            android:label="@string/title_activity_place_order"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
